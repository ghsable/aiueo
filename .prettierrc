{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": as-needed,
  "jsxSingleQuote": false,
  "trailingComma": none,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": always,
  "proseWrap": never,
  "htmlWhitespaceSensitivity": css,
  "endOfLine": auto,
  "embeddedLanguageFormatting": auto
}
